{
  "name": "teamkeo-tourney-banpick",
  "version": "1.0.0",
  "description": "Tournament Ban/Pick System for Team KEO",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
